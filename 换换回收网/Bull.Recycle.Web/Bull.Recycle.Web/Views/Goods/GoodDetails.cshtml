        var Recode = {
            second: 0,
            minute: 0,
            hour: 0,
            stoptime: 0,
            clicknum: 0,
            url: "/Burypoint/inserRecodData",
            address: returnCitySN['cname'],
            ip: returnCitySN['cip'],
            locationurl: $.cookie("locationUrl"),
            curl: "估价页",
            self_url: window.location.href,
            recordInterval: function (obj) {
                obj.second++;
                if (obj.second == 60) {
                    obj.second = 0;
                    obj.minute += 1;
                }
                if (obj.minute == 60) {
                    obj.minute = 0;
                    obj.hour += 1;
                }

                obj.stoptime = obj.hour + "时" + obj.minute + "分" + obj.second + "秒";

            },
            postRecodeData: function (url, data) {
                $.ajax({
                    type: "POST",
                    url: url,
                    data: data,
                    success: function (msg) {

                    }
                });
            },
            makepostData: function () {
                var thisobj = this;
                var postdata = {
                    'address': thisobj.address,
                    'stoptime': thisobj.stoptime,
                    'second': thisobj.second,
                    "clicknum": thisobj.clicknum,
                    'locationurl': thisobj.locationurl,
                    'self_url': thisobj.self_url,
                    'ip': thisobj.ip,
                    'curl': thisobj.curl
                };
                return postdata;
            },
            initialize: function () {

                if (this.locationurl) {
                    setInterval(this.recordInterval, 1000, this);
                    var thisobj = this;
                    $(document).click(function () {
                        thisobj.clicknum += 1;
                    });


                    //监听浏览器关闭事件
                    window.onbeforeunload = function (e) {
                        var data = thisobj.makepostData();
                        thisobj.postRecodeData(thisobj.url, data);
                    };

                    //这是监听浏览器回退事件
                    $(window).on('popstate', function () {
                        var data = thisobj.makepostData();
                        thisobj.postRecodeData(thisobj.url, data);
                    });
                    window.history.pushState('forward', null, '#'); //在IE中必须得有这两行
                    window.history.forward(1);
                }
            }
        };
        Recode.initialize();
    </script>

